/* pages/report/report.wxss */
.tabs{
  width:90%;
  display: block; 
  box-sizing: border-box; 
  position: relative;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  box-sizing: border-box;
}

.background {
  width: 140%;
  height: 100%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: -1;
}

.report {
  display: flex;
  width: 100%;
  margin-top: 20px;
}